@model Garage2._0.Models.ParkedVehicle

@{
    ViewBag.Title = "Kvitto";
}

<h2>@ViewBag.Title för @Html.DisplayFor(model => model.RegNumber)</h2>

<div>
    @*<h4>kvitto</h4>*@
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.RegNumber)</dt>
        <dd>@Html.DisplayFor(model => model.RegNumber)</dd>

        <dt>@Html.DisplayNameFor(model => model.TypeOfVehicle)</dt>
        <dd>@Html.DisplayFor(model => model.TypeOfVehicle)</dd>

        <dt>Check-in tid</dt>
        <dd>@Html.DisplayFor(model => model.CheckInTime)</dd>

        <dt>Check-ut tid</dt>
        <dd>@ViewBag.CheckOutTime</dd>

        <dt>Parkeringstid</dt>
        <dd>@ViewBag.ParkingDuration</dd>

        <dt>Parkeringsavgift</dt>
        <dd>@ViewBag.ParkingFee kronor</dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
